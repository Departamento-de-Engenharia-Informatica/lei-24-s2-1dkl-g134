<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="453.0" prefWidth="686.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ipp.isep.dei.esoft.project.ui.gui.RegisterGreenSpaceUI">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Menus...">
          <items>
            <MenuItem fx:id="toStart" mnemonicParsing="false" text="Start" />
            <MenuItem mnemonicParsing="false" text="Register Green Space" />
            <MenuItem mnemonicParsing="false" onAction="#toUS27" text="List Green Spaces managed by you" />
            <MenuItem mnemonicParsing="false" onAction="#toUS21" text="Add Task to To-Do List" />
            <MenuItem mnemonicParsing="false" onAction="#toUS22" text="Add Task to Agenda" />
            <MenuItem mnemonicParsing="false" onAction="#toUS23" text="Assign Team to Task" />
            <MenuItem mnemonicParsing="false" onAction="#toUS26" text="Assign Vehicles to Task" />
            <MenuItem mnemonicParsing="false" onAction="#toUS24" text="Postpone Task" />
                  <MenuItem mnemonicParsing="false" onAction="#toUS25" text="Cancel Task" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="28.0" layoutY="27.0" prefHeight="46.0" prefWidth="502.0" text="Register a new Green Space">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Button layoutX="421.0" layoutY="341.0" mnemonicParsing="false" onAction="#registerGreenSpace" prefHeight="46.0" prefWidth="219.0" text="Register Green Space">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Label layoutX="436.0" layoutY="278.0" prefHeight="63.0" prefWidth="188.0" text="Fill out every field, select a green space type, then press the button below to register it." wrapText="true" />
            <TextField fx:id="name" layoutX="130.0" layoutY="137.0" prefHeight="25.0" prefWidth="203.0" />
            <Label alignment="CENTER_RIGHT" layoutX="56.0" layoutY="131.0" prefHeight="37.0" prefWidth="64.0" text="Name" textAlignment="RIGHT">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TextField fx:id="address" layoutX="130.0" layoutY="202.0" prefHeight="25.0" prefWidth="203.0" />
            <Label alignment="CENTER_RIGHT" layoutX="38.0" layoutY="196.0" prefHeight="37.0" prefWidth="82.0" text="Address" textAlignment="RIGHT">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TextField fx:id="area" layoutX="130.0" layoutY="266.0" prefHeight="25.0" prefWidth="203.0" />
            <Label alignment="CENTER_RIGHT" layoutX="56.0" layoutY="260.0" prefHeight="37.0" prefWidth="64.0" text="Area" textAlignment="RIGHT">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label layoutX="333.0" layoutY="266.0" prefHeight="25.0" prefWidth="64.0" text="hectares">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
            <ListView fx:id="types" editable="true" layoutX="436.0" layoutY="118.0" prefHeight="142.0" prefWidth="151.0" />
            <Label layoutX="436.0" layoutY="88.0" prefHeight="25.0" prefWidth="64.0" text="Type" textAlignment="RIGHT">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </children>
    </AnchorPane>
  </children>
</VBox>
