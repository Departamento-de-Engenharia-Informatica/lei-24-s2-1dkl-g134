<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="453.0" prefWidth="686.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ipp.isep.dei.esoft.project.ui.gui.MainAdminUI">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Menus...">
          <items>
            <MenuItem mnemonicParsing="false" text="Start" />
            <MenuItem mnemonicParsing="false" onAction="#toUS20" text="Register Green Space" />
            <MenuItem mnemonicParsing="false" onAction="#toUS27" text="List Green Spaces managed by you" />
            <MenuItem mnemonicParsing="false" onAction="#toUS21" text="Add Task to To-Do List" />
            <MenuItem mnemonicParsing="false" onAction="#toUS22" text="Add Task to Agenda" />
            <MenuItem mnemonicParsing="false" onAction="#toUS23" text="Assign Team to Task" />
            <MenuItem mnemonicParsing="false" onAction="#toUS26" text="Assign Vehicles to Task" />
            <MenuItem mnemonicParsing="false" onAction="#toUS24" text="Postpone Task" />
                  <MenuItem mnemonicParsing="false" onAction="#toUS25" text="Cancel Task" />
                  <MenuItem mnemonicParsing="false" onAction="#toUS1" text="Register Skill" />
                  <MenuItem mnemonicParsing="false" onAction="#toUS2" text="Register Job" />
                  <MenuItem mnemonicParsing="false" onAction="#toUS3" text="Register Collaborator" />
                  <MenuItem mnemonicParsing="false" onAction="#toUS4" text="Assign Skills to Collaborator" />
                  <MenuItem mnemonicParsing="false" onAction="#toUS5" text="Generate Teams" />
                  <MenuItem mnemonicParsing="false" onAction="#toUS6" text="Register Vehicle" />
                  <MenuItem mnemonicParsing="false" onAction="#toUS7" text="Register Checkup" />
                  <MenuItem mnemonicParsing="false" onAction="#toUS8" text="List Vehicles Requiring Checkup" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="title" layoutX="97.0" layoutY="60.0" prefHeight="85.0" prefWidth="492.0" text="Welcome to GreenGuardian!">
               <font>
                  <Font size="39.0" />
               </font>
            </Label>
            <Label alignment="CENTER" layoutX="219.0" layoutY="161.0" prefHeight="53.0" prefWidth="248.0" text="Administrator Menu" textAlignment="CENTER">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Label alignment="TOP_CENTER" layoutX="102.0" layoutY="250.0" prefHeight="122.0" prefWidth="209.0" text="Here you will find various functionalities that will aid you in the performance of your duties as an Administrator or Manager in the maintenance and management of green spaces that fall under your responsibility." textAlignment="CENTER" wrapText="true" />
            <Label alignment="TOP_CENTER" layoutX="380.0" layoutY="250.0" prefHeight="94.0" prefWidth="209.0" text="Press the &quot;Menus...&quot; button on the top bar to view all available functionalities. Press each option to be taken to that window to perform that function." textAlignment="CENTER" wrapText="true" />
         </children>
    </AnchorPane>
  </children>
</VBox>
